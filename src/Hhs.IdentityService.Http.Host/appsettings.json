{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore":"Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "EventBus": {
      "ExchangeName": "HHS",
      "ConsumerClientName": "IdentityService",
      "ErrorClientName": "EventManagerService",
      "EventNamePrefix": "",
      "EventNameSuffix": "Eto"
    }
  },
  "RabbitMQ": {
    "EventBus": {
      "ExchangeName": "HHS",
      "ConsumerClientName": "IdentityService",
      "ErrorClientName": "EventManagerService",
      "EventNamePrefix": "",
      "EventNameSuffix": "Eto",
      "ConsumerMaxFetchCount": 1,
      "ConsumerParallelThreadCount": 1
    }
  },
  "MicroserviceSettings": {
    "IsActiveResponseDataManipulation": true,
    "IgnoreNullValueForJsonResponse": false,
    "RequestResponseLogger": {
      "IsEnabled": false,
      "Name": "HHS_IdentityService",
      "DateTimeFormat": "yyyy-MM-dd HH:mm:ss"
    }
  },
  "FakeSettings": {
    "OperationSucceeded": true
  }
}

