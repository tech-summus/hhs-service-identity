{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Debug",
      "Microsoft.Hosting.EntityFrameworkCore": "Information"
    }
  },
  "FrameworkLogger": {
    "LogLevel": "Debug"
  },
  "ConnectionStrings": {
    "EfCore-IdentityService": "Host=hhs-infra-postgres;Port=5432;Database=HHS_IdentityService;User ID=postgres;Password=postgres;Pooling=true;Connection Lifetime=0;"
  },
  "Kafka": {
    "Connection": {
      "HostName": "hhs-infra-kafka",
      "Port": 9092
    },
    "EventBus": {
      "KafkaProducerConfig": {
        "ReceiveMessageMaxBytes": "50000000",
        "MessageMaxBytes": "50000000"
      },
      "KafkaConsumerConfig": {
        "SessionTimeoutMs": "90000",
        "HeartbeatIntervalMs": "30000",
        "FetchMaxBytes": "50000000",
        "MaxPartitionFetchBytes": "50000000"
      }
    }
  },
  "RabbitMQ": {
    "Connection": {
      "HostName": "hhs-infra-rabbitmq", // docker-container-name
      "Port": 5672, // docker-default-port
      "UserName": "guest",
      "Password": "guest"
    }
  }
}
